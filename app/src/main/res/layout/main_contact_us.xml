<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/blue"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp">
    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_height="200dp"
        android:layout_margin="5dp"
        android:elevation="5dp"
        card_view:cardCornerRadius="2dp"
        card_view:contentPadding="10dp">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">


            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textSize="30dp"
                android:textColor="@color/green"
                android:text="Prof. Kiron K R"
                android:layout_alignParentTop="true"/>

            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green"
                android:layout_marginTop="10dp"
                android:text="Convener"
                android:textSize="15dp"
                android:layout_below="@+id/textView"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/lyout_kiran_number"
                android:layout_below="@+id/textView2">
            <ImageView
                android:layout_width="40sp"
                android:id="@+id/call_contact"
                android:paddingTop="10sp"
                android:layout_height="40sp"
                android:src="@drawable/call"/>
            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/contact_number"
                android:layout_toRightOf="@+id/call_contact"
                android:layout_below="@+id/textView2"
                android:textColor="@color/green"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10sp"
                android:autoLink="phone"
                android:textColorLink="@color/green"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="+91 94963 29746"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/lay_kiron"
                android:layout_below="@+id/lyout_kiran_number">
            <ImageView
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:id="@+id/mail_at"
                android:paddingTop="10sp"
                android:src="@drawable/mail"/>
            <com.phacsin.entreprenia.customfonts.MyTextView

                android:id="@+id/contact_mail"
                android:layout_toRightOf="@+id/mail_at"
                android:layout_below="@+id/contact_number"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green"
                android:text="kiron@entreprenia.org"/>
            </LinearLayout>
        </RelativeLayout>
    </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/card_view4"
            android:layout_gravity="center"
            android:layout_width="fill_parent"
            android:layout_height="200dp"
            android:background="@color/transparent"
            android:layout_margin="2dp"
            android:elevation="5dp"

            card_view:cardCornerRadius="2dp"
            card_view:contentPadding="10dp">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <com.phacsin.entreprenia.customfonts.MyTextView
                    android:id="@+id/jyothish"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="30dp"
                    android:text="Jyothish Krishna"
                    android:textColor="@color/green"
                    android:layout_alignParentTop="true"/>

                <com.phacsin.entreprenia.customfonts.MyTextView
                    android:id="@+id/pani_jyothish"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="Public Relation Officer"
                    android:textColor="@color/green"
                    android:textSize="15dp"
                    android:layout_below="@+id/jyothish"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/pani_jyothish"
                    android:id="@+id/lay_jyo">
                    <ImageView
                        android:layout_width="40sp"
                        android:id="@+id/jyo_contact"
                        android:paddingTop="10sp"
                        android:layout_below="@+id/pani"
                        android:layout_height="40sp"
                        android:src="@drawable/call"/>
                    <com.phacsin.entreprenia.customfonts.MyTextView
                        android:id="@+id/jyo_number"
                        android:layout_toRightOf="@+id/febin_contact"
                        android:layout_below="@+id/pani"
                        android:textColor="@color/green"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="10sp"
                        android:autoLink="phone"
                        android:textColorLink="@color/green"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="+919447930838"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lay_jyo">
                    <ImageView
                        android:layout_width="40sp"
                        android:layout_height="40sp"
                        android:id="@+id/mail_jyo"
                        android:paddingTop="10sp"
                        android:layout_below="@+id/febin_contact"
                        android:src="@drawable/mail"/>
                    <com.phacsin.entreprenia.customfonts.MyTextView
                        android:id="@+id/contact_jyo"
                        android:layout_toRightOf="@+id/mail_jyo"
                        android:layout_below="@+id/jyo_number"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="10sp"
                        android:textColor="@color/green"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="febin@entreprenia.org"/>
                </LinearLayout>
            </RelativeLayout>
        </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/card_view2"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_height="200dp"
        android:background="@color/transparent"
        android:layout_margin="2dp"
        android:elevation="5dp"

        card_view:cardCornerRadius="2dp"
        card_view:contentPadding="10dp">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/febin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textSize="30dp"
                android:text="Febin Jishal"
                android:textColor="@color/green"
                android:layout_alignParentTop="true"/>

            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/pani"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="Student Coordinator"
                android:textColor="@color/green"
                android:textSize="15dp"
                android:layout_below="@+id/febin"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pani"
                android:id="@+id/lay_febin">
            <ImageView
                android:layout_width="40sp"
                android:id="@+id/febin_contact"
                android:paddingTop="10sp"
                android:layout_below="@+id/pani"
                android:layout_height="40sp"
                android:src="@drawable/call"/>
            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/febin_number"
                android:layout_toRightOf="@+id/febin_contact"
                android:layout_below="@+id/pani"
                android:textColor="@color/green"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10sp"
                android:autoLink="phone"
                android:textColorLink="@color/green"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="+918907905554"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lay_febin"
                android:clickable="true">
            <ImageView
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:id="@+id/mail_febin"
                android:paddingTop="10sp"
                android:layout_below="@+id/febin_contact"
                android:src="@drawable/mail"/>
            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/contact_febin"
                android:layout_toRightOf="@+id/mail_febin"
                android:layout_below="@+id/febin_number"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10sp"
                android:textColor="@color/green"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="jyothish@entreprenia.org"/>
            </LinearLayout>
        </RelativeLayout>
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/card_view3"
        android:layout_gravity="center"
        android:layout_width="fill_parent"
        android:layout_height="200dp"
        android:background="@color/transparent"
        android:elevation="5dp"
        android:layout_margin="5dp"
        card_view:cardCornerRadius="2dp"
        card_view:contentPadding="10dp">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/hari"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:textSize="30dp"
                android:text="Harikrishnan R"
                android:textColor="@color/green"
                android:layout_alignParentTop="true"/>

            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/pani1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:textColor="@color/green"
                android:text="Student Advisor,IEDC NSSCE"
                android:textSize="15dp"
                android:layout_below="@+id/hari"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pani1"
                android:id="@+id/lay_hari">
            <ImageView
                android:layout_width="40sp"
                android:id="@+id/hari_contact"
                android:paddingTop="10sp"
                android:layout_below="@+id/pani1"
                android:layout_height="40sp"
                android:src="@drawable/call"/>
            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/hari_number"
                android:layout_toRightOf="@+id/hari_contact"
                android:layout_below="@+id/pani1"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autoLink="phone"
                android:textColorLink="@color/green"
                android:textColor="@color/green"
                android:text="+91 96058 71373"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lay_hari">
            <ImageView
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:id="@+id/mail_hari"
                android:paddingTop="10sp"
                android:layout_below="@+id/hari_contact"
                android:src="@drawable/mail"/>
            <com.phacsin.entreprenia.customfonts.MyTextView
                android:id="@+id/contact_hari"
                android:layout_toRightOf="@+id/mail_hari"
                android:layout_below="@+id/hari_number"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green"
                android:text="hari@entreprenia.org"/>
            </LinearLayout>
        </RelativeLayout>
    </android.support.v7.widget.CardView>
    </LinearLayout>

</ScrollView>